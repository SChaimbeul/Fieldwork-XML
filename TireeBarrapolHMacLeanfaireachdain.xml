         <entry xml:id="TireeandMull_TireeBarrapolHMacLeanfaireachdain-d0e357">
            <form>
               <orth>Tha smuid air.</orth>
            </form>
         </entry>
         <entry xml:id="TireeandMull_TireeBarrapolHMacLeanfaireachdain-d0e363">
            <form>
               <orth>Tha e air an daorach.</orth>
            </form>
         </entry>
         <entry xml:id="TireeandMull_TireeBarrapolHMacLeanfaireachdain-d0e369">
            <form>
               <orth>Tha e air an deoch.</orth>
            </form>
         </entry>
         <entry xml:id="TireeandMull_TireeBarrapolHMacLeanfaireachdain-d0e375">
            <form>
               <orth>Ghabh e ’n daorach.</orth>
            </form>
         </entry>
         <entry xml:id="TireeandMull_TireeBarrapolHMacLeanfaireachdain-d0e381">
            <form>
               <orth>Ghabh e ’n deoch.</orth>
            </form>
         </entry>
         <entry xml:id="TireeandMull_TireeBarrapolHMacLeanfaireachdain-d0e387">
