            <entry xml:id="Agriculture_anonymousagriculture-d0e267">
              <form>
                <orth>braìdh</orth>
              </form>
              <def>collar</def>
            </entry>
            <entry xml:id="Agriculture_anonymousagriculture-d0e276">
              <form>
                <orth>sraher</orth>
              </form>
              <def>(f) saddle</def>
            </entry>
            <entry xml:id="Agriculture_anonymousagriculture-d0e285">
              <form>
                <orth>brekish</orth>
              </form>
              <def>(f) britches</def>
            </entry>
            <entry xml:id="Agriculture_anonymousagriculture-d0e294">
              <form>
                <orth>?</orth>
              </form>
              <def>blinders</def>
            </entry>
          </div>
          <div>
            <head xml:id="Agriculture_anonymousagriculture-d0e302" type="h3">For ploughing:</head>
            <entry xml:id="Agriculture_anonymousagriculture-d0e307">
              <form>
                <orth>braìdh</orth>
              </form>
              <def>collar</def>
            </entry>
            <entry xml:id="Agriculture_anonymousagriculture-d0e316">
