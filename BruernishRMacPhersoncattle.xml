      <entry xml:id="AssortedFieldworkers_BruernishRMacPhersoncattle-d0e575">
        <form>
          <orth>a’ fiarach</orth>
        </form>
        <def>grazing.</def>
      </entry>
      <entry xml:id="AssortedFieldworkers_BruernishRMacPhersoncattle-d0e584">
        <form>
          <orth>a’ cuallach</orth>
        </form>
        <def>driving home slowly, giving time to eat on way.</def>
      </entry>
      <entry xml:id="AssortedFieldworkers_BruernishRMacPhersoncattle-d0e593">
        <form>
          <orth>a’ ciùilleadh</orth>
        </form>
        <def>driving quickly.</def>
      </entry>
