         <entry xml:id="WoolworkingandFishing_SiabostMJMacLeodiasgach-d0e84">
            <form>
               <orth>mogul</orth>
            </form>
            <def>mesh.</def>
         </entry>
         <head
             xml:id="WoolworkingandFishing_SiabostMJMacLeodiasgach-d0e92"
