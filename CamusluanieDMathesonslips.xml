      <entry xml:id="Slips_CamusluanieDMathesonslips-idp161847664">
        <form>
          <orth>leth-tithich</orth>
          <pron>[lʹɛtʹiiç]</pron>
        </form>
        <def>[lʹɛtʹiiç] Notes: gullet.</def>
      </entry>
      <entry xml:id="Slips_CamusluanieDMathesonslips-idp161844496">
        <form>
          <orth>athairle</orth>
